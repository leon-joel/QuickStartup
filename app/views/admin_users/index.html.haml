%h1 Listing admin_users

%table
  %thead
    %tr
      %th Name
      %th
      %th
      %th

  %tbody
    - @admin_users.each do |admin_user|
      %tr
        %td= admin_user.name
        %td= link_to 'Show', admin_user
        %td= link_to 'Edit', edit_admin_user_path(admin_user)
        %td= link_to 'Destroy', admin_user, :method => :delete, :data => { :confirm => 'Are you sure?' }

%br

= link_to 'New Admin user', new_admin_user_path
